re\" },\n        { text: \"Quantity\", value: \"quantity\" },\n        { text: \"Price\", value: \"price\" },\n      ],\n      dialogVisible: false,\n      selectedBook: {},\n    };\n  },\n  methods: {\n    editBook(book) {\n      this.selectedBook = book;\n      this.dialogVisible = true;\n    },\n    addBook() {\n      this.dialogVisible = true;\n    },\n    async refreshList() {\n      this.dialogVisible = false;\n      this.selectedBook = {};\n      this.books = await api.books.allBooks();\n    },\n    userManagement(){\n      router.push(\"./users\");\n    },\n  },\n  created() {\n    this.refreshList();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}