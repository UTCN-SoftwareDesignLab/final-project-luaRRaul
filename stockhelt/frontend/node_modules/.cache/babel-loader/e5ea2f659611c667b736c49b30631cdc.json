dmin\");\n    },\n    editUser(user) {\n      this.selectedUser = user;\n      this.dialogVisible = true;\n    },\n    addUser() {\n      this.dialogVisible = true;\n    },\n    async refreshList() {\n      this.dialogVisible = false;\n      this.selectedUser = {};\n      this.users = await api.users.allUsers();\n    },\n  },\n  created() {\n    this.refreshList();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}