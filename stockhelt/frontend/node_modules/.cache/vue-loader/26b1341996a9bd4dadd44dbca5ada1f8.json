 uuid\n      // Reset the text input\n      if(this.$store.getters[\"auth/isDoctor\"]) {\n        this.$root.$emit(\"doctorSendMessage\",event.target.value);\n      }else{\n        this.$root.$emit(\"patientSendMessage\",event.target.value);\n      }\n      event.target.value = '';\n    }\n  },\n};\n</script>\n\n<style scoped>\n.message-input {\n  display: table-row;\n  width: 100%;\n  height: 26px;\n}\ntextarea {\n  width: 98%;\n  height: 30px;\n  padding: 0 5px;\n  margin: 0;\n  box-sizing: border-box;\n  line-height: 20pt;\n  resize: none;\n  outline: none;\n  font-size: 14px;\n  border: solid 1px #AAAAAA;\n  border-radius: 3px;\n  font-family: Helvetica;\n}\n</style>\n"]}]}